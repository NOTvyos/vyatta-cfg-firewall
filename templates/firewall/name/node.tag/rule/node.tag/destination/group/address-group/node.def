type: txt
help: Group of addresses

commit:expression: exec "sudo /opt/vyatta/sbin/vyatta-ipset.pl \
                           --action=check-set-type        \
                           --set-name=$VAR(@)             \
                           --set-type=address;"

allowed: local -a array
         array=( /opt/vyatta/config/active/firewall/group/address-group/* )
         echo -n ${array[@]##*/}
