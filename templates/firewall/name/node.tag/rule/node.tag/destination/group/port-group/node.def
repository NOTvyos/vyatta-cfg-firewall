type: txt
help: Set group of ports

commit:expression: exec "sudo /opt/vyatta/sbin/vyatta-ipset.pl \
                           --action=check-set-type        \
                           --set-name=$VAR(@)             \
                           --set-type=port;"
allowed: local -a array
         array=( /opt/vyatta/config/active/firewall/group/port-group/* )
         echo -n ${array[@]##*/}
