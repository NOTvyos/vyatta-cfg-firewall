type: txt
help: Group of networks

commit:expression: exec "sudo /opt/vyatta/sbin/vyatta-ipset.pl \
                           --action=check-set-type        \
                           --set-name=$VAR(@)             \
                           --set-type=network;"
allowed: local -a array
         array=( /opt/vyatta/config/active/firewall/group/network-group/* )
         echo -n ${array[@]##*/}
